{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase" : "off",
        "no-unused-vars" : "off",
        "no-sequences" : "off",
        "no-unused-expressions" : "off",
        "no-undef" : "off"
    }
}
